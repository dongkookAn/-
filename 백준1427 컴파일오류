import java.util.Arrays;
import java.util.Scanner;
public class Main {
 
	
	
	public static void main(String[] args) {
 
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		char arr[]=new char[a];
		
		for (int i=0;i<a;i++) {
			arr[i]=Integer.toString(a).charAt(i);
		}
		Arrays.sort(arr);
		
		for(int i=0;i<a;i++) {
			System.out.print(arr);
			}
		
		}
		
	}
