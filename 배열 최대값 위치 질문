import java.util.Scanner;

public class Main {

	public static void main(String[] args)  {
		
		Scanner sc = new Scanner (System.in);
		int arr[] = new int[9];
		int max=0;
		int maxindex=1;
		
		
		for(int i =0;i<9;i++) {
			arr[i]= sc.nextInt();
			
		}
		for(int k=1;k<10;k++) {
			if(arr[k-1]>max)   {
				max = arr[k-1];
				maxindex = k;

				
		}
		}
		
		System.out.println(maxindex);
		}
		

	}
