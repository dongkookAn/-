import java.util.Scanner;

public class Main {

	public static void main(String[] args)  {
		
		Scanner sc = new Scanner (System.in);
		
		int a = sc.nextInt();
		int min=1000000;
		int max=0;
		
		int arr [] = new int [a];
		
		for(int i=0;i<a;i++) {
			arr[i] = sc.nextInt();
			
			
			
		}
		for(int k=0 ; k<a ; k++) {
			if(arr[k]<min) {
				min = arr[k];}
			if(arr[k]>max) {
				max = arr[k];}
				
			}
		
		System.out.println(min);
		System.out.println(max);
		}
		

	}
