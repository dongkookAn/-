import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	 Scanner sc = new Scanner(System.in);
	 
	  int a = sc.nextInt();
	  int arr[] = new int[a];
	  int count =0;
	  for(int i=0;i<a;i++) {
		  arr[i]=sc.nextInt();
		
				
			}
	  
	  for(int i=a-1;i<a; i++) {
		  for(int j=0;j<a;j++) {
			  if(i!=j) {
				  if(arr[i]==arr[j]) {
					  
					  count++;
					  
					  
				  }
			  }
		  }
	  }
	  sc.close();
			System.out.println(count);
		}
	  
		}
