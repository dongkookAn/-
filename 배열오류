import java.util.Scanner;

public class Main {

	

	public static void main(String[] args) {

int a;
int b;

Scanner sc = new Scanner (System.in);

int arr[i];
for(int i=0 ; i<=5 ; i++ ) {
	a=sc.nextInt();
	b=sc.nextInt();
    arr[i] = a+b;
	
	
}

for(int i=0 ; i<=5 ; i++ ) {
