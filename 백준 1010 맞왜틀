import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	 Scanner sc = new Scanner(System.in);
	 
	  int n = sc.nextInt();
	  int m = sc.nextInt();
	  int sum=1;
	  
	  while(true) {
		  
	  
		  
		  sum *= m-n+1;
		  m--;
		  n--;
		  if(n==0) {break;
		  }
	  }
	  System.out.println(sum);
		}
	  
		}
